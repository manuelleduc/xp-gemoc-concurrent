	mapping inMM1::Block::blockStatementsOrdered12blockStatementsOrdered()
	{
		init{
			var sources : Sequence(Statement) := self.blockStatements->asSequence();
			var i : Integer := 1;
			var j : Integer := 1;
			var size : Integer = sources->size(); 
			while (i <= size){
				j := i+1;
				while (j <= size){
					theMainBlock.relations +=  self.map intermediate_blockStatementsOrdered12blockStatementsOrdered(sources->at(i),sources->at(j), i, j);
					j := j + 1;
				};
				i := i + 1;
			};
		}
	}

		
	
